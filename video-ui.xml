<mythuitheme>

    <!-- Home -> Media Library -> Watch Videos -> select video, 'I'(nfo)
	 -> Video Info -> View Cast
    -->
    <window name="castpopup">
        <area>-1,-1,600,900</area>

	<shape name="background" from="basewidgetshape" />

        <buttonlist name="cast" from="basepopupbuttonlist">
	    <area>20,20,-20,-100</area>
	</buttonlist>

        <button name="ok" from="varbuttonright">
            <value>Done</value>
        </button>
    </window>

    <!-- Home -> Media Library -> Watch Videos -> select video, 'I'(nfo)
	 -> Video Info -> View Full Plot
    -->
    <window name="descriptionpopup">
        <area>-1,-1,1280,720</area>

	<shape name="background" from="basewidgetshape" />

        <textarea name="description" from="basetextarea">
            <area>20,20,-20,-100</area>
            <multiline>yes</multiline>
        </textarea>

        <button name="ok" from="varbuttonright">
            <value>Done</value>
        </button>
    </window>

    <!-- Home -> Media Library -> Watch Videos -> select video, 'I'(nfo)
	 -> Video Info -> View Details
    -->
    <window name="itemdetailpopup">
        <area>-1,-1,1280,800</area>

	<shape name="basewidgetshape">
	    <area>0,0,100%,100%</area>
	    <type>roundbox</type>
	    <cornerradius>20</cornerradius>
	    <fill color="#000000" alpha="255" />
	    <line color="#FFFFFF" width="6" alpha="96"/>
	</shape>

        <imagetype name="coverart">
            <area>25,40,312,465</area>
            <filename>Images/Graphite/mv_browse_nocover_large.png</filename>
            <preserveaspect>true</preserveaspect>
        </imagetype>

	<group name="previewgroup">
	    <area>25,520,434,255</area>
	    <minsize vanish="yes">100%,100%</minsize>

	    <shape name="previewShape">
		<area>0,0,100%,100%</area>
		<minsize vanish="yes">100%,100%</minsize>
		<type>roundbox</type>
		<cornerradius>5</cornerradius>
		<fill color="#555555" alpha="255" />
		<line color="#FFFFFF" alpha="160" width="8" />
	    </shape>

	    <imagetype name="screenshot">
		<area>12,12,-12,-12</area>
		<minsize vanish="yes">100%,100%</minsize>
		<preserveaspect>true</preserveaspect>
	    </imagetype>
	</group>


        <textarea name="title" from="basetextarea">
