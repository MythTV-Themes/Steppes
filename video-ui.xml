        </textarea>

	<textarea name="subtitle">
	    <area>350,90,-15,80</area>
	    <font>mymedium</font>
	    <multiline>yes</multiline>
	    <align>center</align>
	</textarea>

        <textarea name="description" from="basetextarea">
            <area>370,170,-15,345</area>
            <multiline>yes</multiline>
        </textarea>

        <textarea name="rating" from="basetextarea">
            <area>480,530,770,35</area>
        </textarea>

        <textarea name="year" from="basetextarea">
            <area>710,530,200,35</area>
	    <template>Year: %1</template>
	    <align>top,hcenter</align>
        </textarea>

        <textarea name="userrating" from="basetextarea">
            <area>940,530,220,35</area>
            <align>right,top</align>
            <template>User Rating: %1</template>
        </textarea>

        <textarea name="length" from="basetextarea">
            <area>480,570,200,35</area>
            <align>top,left</align>
        </textarea>

	<textarea name="s##e##" from="year">
	    <position>710,570</position>
	    <template />
	</textarea>

	<textarea name="inetref" from="userrating">
	    <position>940,570</position>
	    <template>IMDB: %1</template>
	</textarea>

