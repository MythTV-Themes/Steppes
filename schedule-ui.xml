		    </textarea>
		</state>
		<state name="selectedactive" from="active"/>
		<state name="selectedinactive" from="active"/>
	    </statetype>
	</buttonlist>

	<textarea name="datetext">
	    <area>535,89,180,42</area>
	    <font>mymediumnarrow</font>
	    <align>center</align>
	</textarea>

