	</imagetype>
	
        <group name="PageHeading" from="basePageHeading">
	    <group name="Heading">
	        <textarea name="text">
		    <value>Scheduled Recordings</value>
		</textarea>
	    </group>
	</group>

	<group name="msggroup" from="baseMsgGroup">
	    <textarea name="status" from="msgText" />
	</group>

        <textarea name="norecordings_info">
            <area>550,210,-550,-200</area>
            <font>mymedium</font>
            <value>You Haven't Scheduled Any Programs To Be Recorded\n
	    To schedule a recording,exit this screen and
	    select 'Program Guide' or 'Program Finder'</value>
            <multiline>yes</multiline>
            <align>center</align>
        </textarea>

	<group name="modegroup" from="baseModeGroup">
	    <textarea name="filter" from="pagemode" />
	</group>

	<buttonlist name="schedules" from="episodeList">
	    <statetype name="buttonitem">
		<state name="active">
		    <textarea name="card">
			<area>30,6,60,35</area>
			<font>mysmallgrey</font>
			<font state="running">mysmallgreen</font>
			<font state="disabled">mysmallgrey</font>
			<font state="error">mysmallred</font>
			<font state="normal">mysmallblue</font>
			<font state="warning">mysmallorange</font>
			<align>hcenter</align>
			<template>(%1)</template>
		    </textarea>
		</state>

		<state name="inactive" from="active" />

		<state name="selectedinactive">
		    <textarea name="rectypestatus">
			<area>100%-700,100%-41,-20,-8</area>
			<align>right</align>
			<font>myverysmall</font>
			<font state="running">myverysmallgreen</font>
			<font state="disabled">myverysmallgrey</font>
			<font state="error">myverysmallred</font>
			<font state="normal">myverysmallblue</font>
			<font state="warning">myverysmallorange</font>
		    </textarea>

		    <textarea name="card">
			<area>30,8,60,35</area>
			<font>mysmallgrey</font>
			<font state="running">mysmallgreen</font>
			<font state="disabled">mysmallgrey</font>
			<font state="error">mysmallred</font>
			<font state="normal">mysmallblue</font>
			<font state="warning">mysmallorange</font>
			<align>hcenter</align>
			<template>(%1)</template>
		    </textarea>
		</state>

		<state name="selectedactive" from="selectedinactive">
		    <statetype name="status" from="itemsSelectedState" />
		</state>
	    </statetype>
	</buttonlist>

	<buttonlist name="groups" from="groupsList" />

    </window>

